#include<iostream>
#include<cstdio>
#include<cstdlib>
using namespace std;

inline int scan()
{
  register char c;
  register int n=0;
  c=getchar_unlocked();
  while( c<'0' || c>'9')
    c=getchar_unlocked();
  while( c>='0' && c<='9' )
    {
      n=n*10+c-'0';
      c=getchar_unlocked();
    }
  return n;
}


